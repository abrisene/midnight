{
  "name": "@acausal/hash-utils",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hashers": {
      "default": "./dist/hashers/index.js",
      "types": "./dist/hashers/index.d.ts"
    },
    "./factory": {
      "default": "./dist/hash-factory/index.js",
      "types": "./dist/hash-factory/index.d.ts"
    },
    "./hash-schema": {
      "default": "./dist/hash-schema/index.js",
      "types": "./dist/hash-schema/index.d.ts"
    }
  },
  "tsup": {
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": true,
    "sourcemap": true,
    "clean": true,
    "bundle": true,
    "minify": true,
    "entry": [
      "src/index.ts",
      "src/hashers/hashers.ts",
      "src/hash-factory/hash-factory.ts",
      "src/hash-schema/hash-schema.ts"
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "test": "pnpm jest --passWithNoTests",
    "test:watch": "pnpm jest --watchAll",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hash-wasm": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@configs/eslint-config": "workspace:*",
    "@configs/prettier-config": "workspace:*",
    "@configs/tsconfig": "workspace:*",
    "@types/jest": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "prettier": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@configs/prettier-config"
}
