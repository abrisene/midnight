      "src/utils/index.ts",
      "src/schemas/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "bundle": false,
    "minify": false
  },
  "scripts": {
    "build:wip": "tsup",
    "dev": "tsup --watch",
    "test-wip": "pnpm jest --passWithNoTests",
    "test:watch-wip": "pnpm jest --watchAll",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "lint:exports": "pnpx @arethetypeswrong/cli",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@dnd-kit/utilities": "^3.2.2",
    "html2canvas": "^1.4.1",
    "jotai": "^2.10.1",
    "tinycolor2": "^1.6.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acausal/types": "workspace:*",
    "@configs/eslint-config": "workspace:*",
    "@configs/prettier-config": "workspace:*",
    "@configs/tailwind-config": "workspace:*",
    "@configs/tsconfig": "workspace:*",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "catalog:",
    "@types/node": "^22.9.0",
    "@types/react": "catalog:react18",
    "@types/tinycolor2": "^1.4.6",
    "eslint": "catalog:",
    "jest": "catalog:",
    "prettier": "catalog:",
